		'@SyliusWebBundle/Resources/assets/jiwen/css/sub-css.css'
	%}
	<link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
	{% endstylesheets %}
{% endblock %}

{% block content %}
<div id="wrap" class="clearfix">
	<!--sidebar-->
	<div class="sidebar-r flue">
    	<div class="side-inner">
        	<p class="tilte-sr"></p>
            <div class="si-rotete-out">
                <ul class="si-rotete">
                    <li><a href="#"><img src="/assets/images/img-recent.jpg" width="68" height="68" alt=""></a></li>
